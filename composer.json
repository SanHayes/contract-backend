  "prefer-stable": true,
  "require": {
    "php": ">=7.4.0",
    "topthink/framework": "^5.1",
    "topthink/think-helper": "^1.0",
    "topthink/think-worker": "2.0.*",
    "topthink/think-migration": "2.0.*",
    "thans/tp-jwt-auth": "^2.1",
    "nesbot/carbon": "^2.66",
    "predis/predis": "^2.1",
    "iexbase/tron-api": "^5.0",
    "allmediaindo/infura": "dev-main",
    "ext-json": "*",
    "ext-bcmath": "*",
    "pnqi/ethereum-client": "^1.0",
